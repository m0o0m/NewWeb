    <script>
function getQueryString(name) {
	var reg = new RegExp("(^|&)" + name + "=([^&]*)(&|$)", "i");
	var r = window.location.search.substr(1).match(reg);
	if (r != null) return unescape(r[2]); return null;
}
function autoCheckUrl(){
	if (getQueryString("isShowGoodApp")=="true") {
		var li=document.getElementById("goodapp");
		li.style.display="block";
	};
	if(getQueryString("wjsTime") && getQueryString("wjsTime")>0){
		var li=document.getElementById("wjs");
		li.style.display="block";
	};
	if (getQueryString("isShowJifenfanli")=="true") {
		var li=document.getElementById("jifenhuan");
		li.style.display="block";
	};
	if (getQueryString("isscramble")=="true") {
		var li=document.getElementById("jifenzhengbanew");
		li.style.display="block";
	};
	if (getQueryString("isShowTexasAWActive")=="true") {
		var li=document.getElementById("wanpailingdali");
		li.style.display="block";
	};
	if (getQueryString("isShow515Qipaijie")=="true") {
		var li=document.getElementById("515Qipaijie");
		li.style.display="block";
	};
	if (getQueryString("pinbishuizuguan")=="true") {
		var li=document.getElementById("shuizuguan");
		li.style.display="none";
	};
	if (getQueryString("isShowchongzhixiangdali")=="true") {
		var li=document.getElementById("chongzhixiangdali");
		li.style.display="block";
	};
	if (getQueryString("isShowGanendafanli")=="true") {
		var li=document.getElementById("ganendafanli");
		li.style.display="block";
	};
	if (getQueryString("isShowZhongqiu")=="true") {
		var li=document.getElementById("zhongqiuhuodong");
		li.style.display="block";
	};

}
window.onload = autoCheckUrl;

